{
  "name": "wordclock",
  "private": true,
  "version": "0.0.1",
  "description": "wordclock",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "clean.build": "rm -rf www && stencil build",
    "start": "stencil build --dev --watch --serve",
    "start.sw": "stencil build --prod --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "generate": "stencil generate",
    "deploy": "npm run build",
    "deploy.firebase": "npm run build && firebase deploy",
    "clean": "rm -rf www",
    "filenamelength": "cd www && find . | awk 'function base(f) { sub(\".*/\", \"\", f); return f; } { print length(base($0)), $0 }'| sort -n"
  },
  "dependencies": {
    "@ionic/core": "^5.2.1",
    "@stencil/core": "^1.14.0"
  },
  "devDependencies": {
    "@stencil/sass": "^1.3.1",
    "workbox-build": "^5.1.3"
  }
}
